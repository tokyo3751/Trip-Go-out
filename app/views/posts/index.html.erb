<h1>Posts#index</h1>
<p>Find me in app/views/posts/index.html.erb</p>
<% @posts.each do |posts| %>
<div>
  <%= link_to post_path(posts.id) do %>
    <%= image_tag posts.get_image %>
  <% end %>
  <p>投稿ユーザー画像：<%= image_tag 'sample-author1.jpg' %></p>
  <p>ニックネーム：<%= posts.user.nickname %></p>
  <p>場所名：<%= posts.place %></p>
  <p>コメント：<%= posts.comment %></p>
</div>
<% end %>